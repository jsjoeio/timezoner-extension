{
  "manifest_version": 2,
  "name": "Timezoner",
  "version": "1.0.0.1",
  "description": "A simple extension to help you coordinate across timezones",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_title": "Timezoner",
    "default_popup": "index.html"
  }
}
